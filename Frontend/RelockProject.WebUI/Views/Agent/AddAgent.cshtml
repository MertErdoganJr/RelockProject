@model AddAgentViewModel

@{
    ViewData["Title"] = "AddAgent";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <label>Personel Adı</label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />
    <label>Personel Ünvanı</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Personel Görseli</label>
    <input type="text" asp-for="AgentImage" class="form-control" />
    <br />
    <label>Sosyal Medya Hesabı 1</label>
    <input type="text" asp-for="SocialMedia1" class="form-control" />
    <br />
    <label>Sosyal Medya Hesabı 2</label>
    <input type="text" asp-for="SocialMedia2" class="form-control" />
    <br />
    <label>Sosyal Medya Hesabı 3</label>
    <input type="text" asp-for="SocialMedia3" class="form-control" />
    <br />
    <label>Sosyal Medya Hesabı 4</label>
    <input type="text" asp-for="SocialMedia4" class="form-control" />
    <br />
    <label>Sosyal Medya Hesabı 5</label>
    <input type="text" asp-for="SocialMedia5" class="form-control" />
    <br />
    <button class="btn mb-1 btn-info btn-lg">Personeli Ekle</button>
</form>
